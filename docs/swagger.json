{
    "swagger": "2.0",
    "info": {
        "description": "A random lottery generator API using Go \u0026 Fiber",
        "title": "golotteryservice",
        "contact": {
            "name": "Aaron Saikovski"
        },
        "license": {
            "name": "MIT"
        },
        "version": "0.1"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/lottery": {
            "get": {
                "description": "Generates the lottery numbers and returns JSON - The request responds to a url matching:  /lottery?maxgames=XX\u0026randnum=XX\u0026maxnumspergame=XX",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "Lottery"
                ],
                "summary": "Lottery number generator",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/ping": {
            "get": {
                "description": "get the status of server.",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Show the status of server - Ping/Pong",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    }
}